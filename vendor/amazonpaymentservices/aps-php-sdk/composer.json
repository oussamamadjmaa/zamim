{
    "name": "amazonpaymentservices/aps-php-sdk",
    "description": "Amazon Payment Services PHP SDK",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^7.5.0",
        "php": "^8.1",
        "ext-openssl": "*",
        "psr/log": "^3.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AmazonPaymentServicesSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Amazon"
        }
    ],
    "minimum-stability": "dev"
}
